<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="100dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView17"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" android:layout_marginTop="80dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/imageView17"
            android:layout_marginLeft="36dp" android:layout_marginStart="36dp"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" app:layout_constraintStart_toEndOf="@+id/logo" android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17" android:textSize="24sp" app:fontFamily="sans-serif"
            android:textStyle="bold"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView25"
            app:layout_constraintStart_toEndOf="@+id/textView3" app:layout_constraintHorizontal_bias="0.89"
            app:layout_constraintEnd_toStartOf="@+id/imageView26"
            app:layout_constraintHorizontal_chainStyle="packed" app:layout_constraintTop_toTopOf="@+id/imageView26"
            app:layout_constraintBottom_toBottomOf="@+id/imageView26" app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView26"
            app:layout_constraintStart_toEndOf="@+id/imageView25" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView27" android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp" android:layout_marginRight="24dp"
            app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            tools:layout_editor_absoluteY="132dp" android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/imageView26" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView28"
            android:scaleType="centerCrop"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/imageView25"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView29"
            app:layout_constraintHorizontal_chainStyle="spread" app:layout_constraintDimensionRatio="w,1:1"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView29"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toEndOf="@+id/imageView28" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView30" app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView30"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toEndOf="@+id/imageView29" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView31" app:layout_constraintTop_toTopOf="@+id/imageView29"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView31"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toEndOf="@+id/imageView30"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView30"
            app:layout_constraintBottom_toBottomOf="@+id/imageView30"/>
</android.support.constraint.ConstraintLayout>